{
    "name": "Bye Bye Bias",
    "version": "0.0.1",
    "description": "Chrome extension for news articles",
    "manifest_version": 2,
    "icons": {
        "16": "./icon16x16.png",
        "32": "./icon32x32.png",
        "48": "./icon48x48.png",
        "128": "./icon128x128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": ["tabs", "activeTab", "declarativeContent", "storage", "http://*/*", "https://*/*"]
}